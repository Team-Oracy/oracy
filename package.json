  "main": "app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "npm-run-all --parallel serve watch:sass",
    "serve": "nodemon --inspect src/app.js",
    "build:sass": "node-sass -r --output-style compressed src/assets/sass/app.sass public/css/app.css -o public/css",
    "watch:sass": "npm run build:sass && npm run build:sass -- -w"
  },
